windows.debug.x86_64 =   "res://addons/convex-gd/target/debug/convex_gd.dll"
windows.release.x86_64 = "res://addons/convex-gd/target/release/convex_gd.dll"
macos.debug =            "res://addons/convex-gd/target/debug/libconvex_gd.dylib"
macos.release =          "res://addons/convex-gd/target/release/libconvex_gd.dylib"
macos.debug.arm64 =      "res://addons/convex-gd/target/debug/libconvex_gd.dylib"
macos.release.arm64 =    "res://addons/convex-gd/target/release/libconvex_gd.dylib"
web.debug.wasm32 =       "res://addons/convex-gd/target/wasm32-unknown-emscripten/debug/convex_gd.wasm"
